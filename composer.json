{
    "name": "itpathsolutions/role-wise-session-manager",
    "description": "Role wise Managed session",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Itpathsolutions\\Sessionmanager\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Archana Soni",
            "email": "archanau@itpathsolutions.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": ">=5.0 <12.0",
        "spatie/laravel-permission": ">=3.0 <7.0",
        "laravel/ui": ">=2.0 <5.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Itpathsolutions\\Sessionmanager\\SessionManagerServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@php -r \"if (class_exists('Spatie\\\\Permission\\\\PermissionServiceProvider')) { echo 'Spatie already installed. Skipping vendor publish.'; } else { system('php artisan vendor:publish --provider=\\\"Spatie\\\\Permission\\\\PermissionServiceProvider\\\" --tag=config'); system('php artisan vendor:publish --provider=\\\"Spatie\\\\LaravelPermission\\\\PermissionServiceProvider\\\" --tag=migrations'); }\"",
            "@php artisan vendor:publish --provider=\"Itpathsolutions\\Sessionmanager\\SessionManagerServiceProvider",
            "@php artisan migrate",
            "@php artisan ui:auth",
            "@php artisan migrate"
        ],
        "pre-uninstall-cmd": [
            "@php artisan package:remove-migration"
        ]
    },
    "version": "1.0.9"
}
